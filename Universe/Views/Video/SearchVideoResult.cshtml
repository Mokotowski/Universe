@model IEnumerable<Universe.Models.Video>

@{
    ViewData["Title"] = "SearchVideoResult";
}

<h1>SearchVideoResult</h1>
<ul>
    @foreach (var video in Model)
    {
        <li>@video.Name </li>
        <a href="@Url.Action("DetailsVideo","Video", new { Id = video.Id })">Szczegóły</a>

    }
</ul>
